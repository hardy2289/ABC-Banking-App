<div class="page-header">
    <h3 align="center">Bank Staff</h3>
   <div class="contextual">
  </div>
</div>
<table class="table table-striped table-bordered table-condensed">
<thead>
  <thead>
    <tr>
      <th colspan="1">Name</th>
      <th colspan="1">Email</th>
      <th colspan="1">Phone Number</th>
      <th colspan="1">Staff Role</th>
      <th colspan="1">Branch</th>
	  <th colspan="1">Update </th>

    </tr>
  </thead>

  <tbody>
    <% obj.each do |employee| %>
      <tr>
        <td><%= employee.staffName %></td>
        <td><%= employee.staffEmail %></td>
        <td><%= employee.staffPhoneNo %></td>
        <td><%= employee.staffRole %></td>
        <td><%= employee.workAtBranch %></td>
        
				<td><%= link_to 'View', employee %>
        <%= link_to 'Edit', edit_employee_path(employee) %>
				<%= link_to 'Delete', employee, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Employee', new_employee_path %>
<div align="center">
<%= will_paginate %>
</div>